{
    "name": "crcms/framework",
    "description": "The CrCms Framework.",
    "keywords": ["crcms", "laravel", "swoole"],
    "license": "MIT",
    "type": "framework",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "crcms/attribute-contract": "^1.0",
        "crcms/repository": "^2.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "CrCms\\Foundation\\": "src/"
        },
        "files": [
            "src/App/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
